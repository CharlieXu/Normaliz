#ifndef QNMZ_VERSION_H
#define QNMZ_VERSION_H

#define QNMZ_VERSION_MAJOR  @LIBNORMALIZ_VERSION_MAJOR@
#define QNMZ_VERSION_MINOR  @LIBNORMALIZ_VERSION_MINOR@
#define QNMZ_VERSION_PATCH  @LIBNORMALIZ_VERSION_PATCH@
#define QNMZ_VERSION        @LIBNORMALIZ_VERSION_STRING@
#define QNMZ_RELEASE (QNMZ_VERSION_MAJOR * 10000 + QNMZ_VERSION_MINOR * 100 + QNMZ_VERSION_PATCH)

namespace libQnormaliz {
inline unsigned int getVersion()
{
    return QNMZ_RELEASE;
}

} //end namespace libQnormaliz

#endif // QNMZ_VERSION_H
